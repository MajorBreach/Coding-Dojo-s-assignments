@model User


<div class="text-center">
    <p></p>
    <form asp-action="CreateUser" asp-controller="Home" method="post">
        <label for="FirstName">FirstName:</label>
        <input asp-for="FirstName" type="text">
        <span asp-validation-for="FirstName"></span>
        <p></p>
        <label for="LastName">LastName:</label>
        <input asp-for="LastName" type="text">
        <span asp-validation-for="LastName"></span>
        <p></p>
        <label for="Email">Email:</label>
        <input asp-for="Email"type="text" id="Email">
        <span asp-validation-for="Email"></span>
        <p></p>
        <label for="Password">Password:</label>
        <input asp-for="Password"type="text" id="ConfirmPassword">
        <span asp-validation-for="Password"></span>
        <p></p>
        <label for="PasswordConfirm">ConfirmPassword:</label>
        <input asp-for="PasswordConfirm"type="text" id="PasswordConfirm">
        <span asp-validation-for="PasswordConfirm"></span>
        <p></p>
        <button>register</button>
        <p></p>
    </form>
</div>